ARG USER
ARG BUILD_OS_VERSION

FROM httpd:2.4
COPY ./html/ /usr/local/apache2/htdocs/index.html

EXPOSE 80

LABEL creator=$USER \
      build.environment.os.version=$BUILD_OS_VERSION

